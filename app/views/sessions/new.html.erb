<% provide(:title, "Log in") %>
<div class="container nav_padding center">
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag sessions_path do %>
  <p>
    <%= password_field_tag :sec_hash, '' ,class: 'form-control' %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>
  </div>
</div>
</div>